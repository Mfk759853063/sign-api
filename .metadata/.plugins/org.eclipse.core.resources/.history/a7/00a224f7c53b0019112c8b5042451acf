package com.porui.hjl.app.device.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.porui.hjl.app.crestron.entity.APPPosition;
import com.porui.hjl.app.device.dao.interfaces.INewApp;
import com.porui.hjl.common.data.mybatis.dao.BaseDaoIMPL;

/**
 * Created by tablv on 2017-09-23.
 * 
 * @Description:类的作用
 */
@Repository("newAppDao")
public class NewAppImpl extends BaseDaoIMPL<APPPosition> implements INewApp {

	@Override
	public List<Map<String, String>> getHikeDeviceMac(String mac) throws Exception {
		// TODO Auto-generated method stub
		return super.getSqlSession().selectList(getStatement("getHikeDeviceMac"), mac);
	}


}
